package com.app.dto;

import com.app.entity.Role;

import lombok.Getter;
import lombok.NoArgsConstructor;
import lombok.Setter;
import lombok.ToString;

@NoArgsConstructor
@Setter
@Getter
@ToString
public class AppointmentDTO 
{
	
	private String workerName;
	
	private String workerField;
	
	private String visitingCharge;
	
	private String address;
	
	private Long userId;
	
	private Long workerId;

	public AppointmentDTO(String workerName, String workerField, String visitingCharge, String address, Long userId) {
		super();
		this.workerName = workerName;
		this.workerField = workerField;
		this.visitingCharge = visitingCharge;
		this.address = address;
		this.userId = userId;
	}
	
	

}
