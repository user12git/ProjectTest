package com.app.entity;

import javax.persistence.*;

import lombok.Getter;
import lombok.NoArgsConstructor;
import lombok.Setter;
import lombok.ToString;

@Entity
@Table(name = "appointments")
@NoArgsConstructor
@Setter@Getter
@ToString
public class Appointment 
{
	@Id
    @GeneratedValue(strategy = GenerationType.IDENTITY)
	@Column(name="id")
    private Long id;

	@Column(name="Worker_Name")
    private String workerName;
	@Column(name="Worker_Field")
    private String workerField;
	@Column(name="Visiting_Charge")
    private String visitingCharge;
	@Column(name="Address")
    private String address;
	@Column(name="userId")
    private Long userId;
	public Appointment(Long id, String workerName, String workerField, String visitingCharge, String address, Long userId) {
		super();
		this.id = id;
		this.workerName = workerName;
		this.workerField = workerField;
		this.visitingCharge = visitingCharge;
		this.address = address;
		this.userId = userId;
	}
	
	
    
    

}
